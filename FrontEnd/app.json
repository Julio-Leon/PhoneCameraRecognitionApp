{
  "expo": {
    "name": "CameraStarterApp",
    "slug": "CameraStarterApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan objects."
      }
    },
    "android": {
      "permissions": ["CAMERA"],
      "package": "com.camerastarterapp"
    }
  }
}
